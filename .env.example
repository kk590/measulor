# Keygen.sh License Integration Configuration
# Copy this file to .env and fill in your actual values

# Keygen Account ID (found in Keygen dashboard settings)
KEYGEN_ACCOUNT_ID=51bb33ef-d469-4c06-ac4b-68b65ce1c647

# Keygen Product Token (create from Keygen dashboard > API Tokens)
# This should be a Product Token with license.read and license.check-in permissions
KEYGEN_PRODUCT_TOKEN=your_product_token_here

# Optional: Flask configuration
FLASK_ENV=production
FLASK_DEBUG=False
